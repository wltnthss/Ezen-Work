            ShortDateFormat.push("  <td class='subject'><div class='subWrap'><p class='cName'>{3}</p><p class='cTit'>{4}{5}</p><span class='icon'>{6}{7}</span></div></td>");
            ShortDateFormat.push("  <td class='pay'>{8}</td>");
            ShortDateFormat.push("  <td class='recently'><em>{9}</em></td>");
            ShortDateFormat.push("</tr>");

            for (var i = 0; i < (TotalLoopB) ; i++) {
                if (i < RsCntB) {
                    Base = ShortDateFormat.join("");
                    Base = Base.replace("{0}", "id='dev_preview__" + aryShortDate[i].alGINo + "'");
                    Base = Base.replace("{1}", aryShortDate[i].strWorkingTerm);
                    Base = Base.replace("{2}", aryShortDate[i].dutyGuCode);
                    Base = Base.replace("{3}", "<a href=\"javascript:GIB_Read('" + aryShortDate[i].alGINo + "'," + aryShortDate[i].skinType + ");\">"+aryShortDate[i].c_name+"</a>");
                    Base = Base.replace("{4}", aryShortDate[i].strIconImg);
                    Base = Base.replace("{5}", aryShortDate[i].strSubjectClass);
                    Base = Base.replace("{6}", "<a href=\"javascript:GIB_Preview('" + aryShortDate[i].alGINo + "','','', '6');\" class='zoomBtn1'>미리보기</a>");
                    Base = Base.replace("{7}", aryShortDate[i].strLink);
                    Base = Base.replace("{8}", aryShortDate[i].strPay);
                    Base = Base.replace("{9}", aryShortDate[i].strEnd_Date);
                    var $Base = $(Base);
                    $Base.appendTo("#shortDate");
                }
                else {
                    var $li = $("<tr></tr>");
                    $li.html("<td colspan = '6'>&nbsp;</td>");
                    $li.appendTo("#shortDate");

                }
            }
        }
        else {
            var $li = $("<tr></tr>");
            $li.html("<td colspan = '5'>&nbsp;</td>");
            $li.appendTo("#shortDate");

        }
    }
</script>
