<script type="text/javascript">
    var Scroll_brandTopLogo = new Array();
    var set_brandTopLogo_width = -140;   //로고 가로길이
    var settimeanimate_brandTopLogo = 500;   //내용 지나가는 속도
    var settimerolling_brandTopLogo = 5000;  //내용 머무는 시간
    LogoDraw(); //랜덤처리

    if(aryLogo.length > 0)
    {
        for (var i = 0; i < aryLogo.length ; i++) {
            Scroll_brandTopLogo[i] = "<a href='" + aryLogo[i].MainbrandTopUrl + "' target='_blank'>" + aryLogo[i].MainBrandTopLogo + "<span>"+ aryLogo[i].Flags_Name +"</span>";
        }
    }


    var maxNum = Scroll_brandTopLogo.length;
    var beforNum, NowNum01, NowNum02, NowNum03, NowNum04, afterNum;
    NowNum01 = 0;

    var Scroll_TopBrand_Logo_Real = new Array();
    var setInterLogo_MainBanner;
    var setInterImg_MainBanner;
    var setActionStat = true;


    mainLogoBanner_Start(); //롤링

    function LogoDraw() {
        aryLogo.sort(function () { return Math.random() * 2 - 1; });

    }


    function stop_mainLogoBanner() {
        clearInterval(setInterLogo_MainBanner);
    }

    function mainLogoBanner_Start() {
        makeNum_MainBanner();
        binding_MainBanner();
    }


    function binding_MainBanner() {
        $("#dev_brandbtn_pre").bind("click", function (e) {
            unbinding_MainBanner();
            move_MainBanner("0");
        });
        $("#dev_brandbtn_next").bind("click", function (e) {
            unbinding_MainBanner();
            move_MainBanner("1");
        });

    }


    function unbinding_MainBanner() {
        $("#dev_brandbtn_pre").unbind("click", null);
        $("#dev_brandbtn_next").unbind("click", null);
    }


    function move_MainBanner(stat) {

        if (setActionStat) {
            setActionStat = false;
            clearInterval(setInterImg_MainBanner);

            if (stat == "0") {  //µÚ·Î
                $("#dev_brandTop_logo").animate({ marginLeft: 0 }, settimeanimate_brandTopLogo);

                if (NowNum01 == 0) {
                    NowNum01 = maxNum - 1;
                } else {
                    NowNum01 = NowNum01 - 1;
                }

            } else {  //¾ÕÀ¸·Î
                $("#dev_brandTop_logo").animate({ marginLeft: set_brandTopLogo_width * 2 }, settimeanimate_brandTopLogo);

                if (NowNum01 == maxNum - 1) {
                    NowNum01 = 0;
                } else {
                    NowNum01 = NowNum01 + 1;
                }
                ;
            }
            clearInterval(setInterImg_MainBanner);
            setInterLogo_MainBanner = setInterval("makeNum_MainBanner();", 600);

        }
    }

    function makeNum_MainBanner() {
        if (NowNum01 == 0) {
            beforNum = maxNum - 1;
        } else {
            beforNum = NowNum01 - 1;
        }

        if (NowNum01 == maxNum - 1) {
            NowNum02 = 0;
        } else {
            NowNum02 = NowNum01 + 1;
        }

        if (NowNum02 == maxNum - 1) {
            NowNum03 = 0;
        } else {
            NowNum03 = NowNum02 + 1;
        }

        if (NowNum03 == maxNum - 1) {
            NowNum04 = 0;
        } else {
            NowNum04 = NowNum03 + 1;
        }

        if (NowNum04 == maxNum - 1) {
            afterNum = 0;
        } else {
            afterNum = NowNum04 + 1;
        }

        Scroll_TopBrand_Logo_Real[0] = Scroll_brandTopLogo[beforNum];
        Scroll_TopBrand_Logo_Real[1] = Scroll_brandTopLogo[NowNum01];
        Scroll_TopBrand_Logo_Real[2] = Scroll_brandTopLogo[NowNum02];
        Scroll_TopBrand_Logo_Real[3] = Scroll_brandTopLogo[NowNum03];
        Scroll_TopBrand_Logo_Real[4] = Scroll_brandTopLogo[NowNum04];
        Scroll_TopBrand_Logo_Real[5] = Scroll_brandTopLogo[afterNum];


        var k = 0;
        $("#dev_brandTop_logo li").each(function () {

            $(this).html(Scroll_TopBrand_Logo_Real[k]);
            k++;
        });

        $("#dev_brandTop_logo").css("marginLeft", set_brandTopLogo_width);



        stop_mainLogoBanner();
        //	setInterImg_MainBanner = setInterval("move_MainBanner('1');",settimerolling_brandTopLogo);

        binding_MainBanner();
        setActionStat = true;
        //]]>
    }

</script>



